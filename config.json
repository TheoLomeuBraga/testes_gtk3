{
    "icon_res": 32,
    "add_pos_y": 0,
    "aways_active": 0,
    "skip_taskbar_hint": 1,
    "centralize": 0,
    "vertical": 1,
    "seconds_to_update_applets": 4,
    "position": "left",
    "components": [
        {
            "type": "launcher",
            "icon": "~/testes_gtk3/built/icons/start.png",
            "name": "start",
            "command": "rofi -show drun -show-icons"
        },
        {
            "type": "launcher",
            "icon": "~/testes_gtk3/built/icons/browser.png",
            "name": "firefox",
            "command": "firefox"
        },
        {
            "type": "launcher",
            "icon": "~/testes_gtk3/built/icons/file.png",
            "name": "pcmanfm",
            "command": "pcmanfm"
        },
        {
            "type": "launcher",
            "icon": "~/testes_gtk3/built/icons/cmd.png",
            "name": "GNOME Terminal",
            "command": "gnome-terminal"
        },
        {
            "type": "clock",
            "icon": "~/testes_gtk3/built/icons/clock.png",
            "command": "pcmanfm"
        },
        {
            "type": "minimizer",
            "icon": "~/testes_gtk3/built/icons/workspace.png",
            "name": "go to workspace"
        }
    ]
}